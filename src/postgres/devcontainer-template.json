                "3.13-trixie",
                "3.12-trixie",
                "3.11-trixie",
                "3.10-trixie",
                "3.14-bookworm",
                "3.13-bookworm",
                "3.12-bookworm",
                "3.11-bookworm",
                "3.10-bookworm"
            ],
